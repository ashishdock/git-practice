package com.springcore.lifecycle;

public class TestingLC {

	private int price;

	public int getPrice() {
		return price;
	}

	public void setPrice(int price) {
		this.price = price;
	}

	@Override
	public String toString() {
		return "TestingLC [price=" + price + "]";
	}

	public TestingLC(int price) {
		super();
		this.price = price;
	}

	public TestingLC() {
		super();
		// TODO Auto-generated constructor stub
	}
	
	
	
	
}
